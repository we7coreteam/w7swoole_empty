{
	"name": "w7/rangine-demo",
	"type": "project",
	"description": "基于 Swoole 的常驻内存型 PHP 高性能框架",
	"license": "Apache-2.0",
	"keywords": [
		"we7",
		"swoole"
	],
	"homepage": "https://www.rangine.com/",
	"authors": [
		{
			"name": "we7team",
			"email": "donknap@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"W7\\App\\": "app/",
			"W7\\Tests\\": "tests/"
		},
		"files": [
			"app/Helper/Functions.php"
		],
		"classmap": [
			"database/seeds",
			"database/migrations"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"W7\\Tests\\" : "tests/"
		}
	},
	"repositories": {
		"1": {
			"type": "path",
			"url": "../w7-rangine"
		},
		"2": {
			"type": "path",
			"url": "../w7-rangine-crontab"
		},
		"3": {
			"type": "path",
			"url": "../w7-rangine-database-tool"
		},
		"4": {
			"type": "path",
			"url": "../w7-rangine-lang"
		},
		"5": {
			"type": "path",
			"url": "../w7-rangine-command"
		},
		"6": {
			"type": "path",
			"url": "../w7-rangine-mq"
		},
		"7": {
			"type": "path",
			"url": "../w7-rangine-mq-cmq"
		},
		"8": {
			"type": "path",
			"url": "../w7-rangine-debugger"
		}
	},
	"require": {
		"w7/rangine": "dev-dev-2.5.0",
		"w7/rangine-crontab": "dev-dev-2.5.0",
		"w7/rangine-database-tool": "dev-dev-2.5.0",
		"w7/rangine-lang": "dev-dev-2.5.0",
		"w7/rangine-command": "dev-dev-2.5.0",
		"w7/rangine-mq": "dev-dev-2.5.0",
		"w7/rangine-mq-cmq": "dev-dev-2.5.0"
	},
	"require-dev": {
		"w7/rangine-debugger": "dev-dev-2.5.0",
		"phpunit/phpunit": "^7.0",
		"w7/php-cs-fixer": "*"
	}
}